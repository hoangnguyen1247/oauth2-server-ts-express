{
    "name": "oauth2-server-ts-express",
    "version": "1.0.0-develop.2",
    "description": "OAuth provider for express",
    "main": "dist/index.js",
    "scripts": {
        "clean:build": "npx shx rm -rf ./dist",
        "build": "npx tsc -p tsconfig.build.json",
        "build:prod": "npx npm-run-all clean:build build",
        "release": "semantic-release",
        "compile": "tsc",
        "type-check": "tsc --noEmit",
        "lint": "jshint .",
        "test": "mocha --recursive test"
    },
    "repository": {
        "type": "git",
        "url": "git://github.com/hoangnguyen1247/oauth2-server-ts-express"
    },
    "keywords": [
        "express",
        "oauth",
        "oauth2"
    ],
    "author": {
        "name": "Nuno Sousa",
        "email": "nfs@seegno.com"
    },
    "license": "SEE LICENSE IN LICENSE",
    "dependencies": {
        "bluebird": "^3.0.5",
        "express": "^4.13.3",
        "oauth2-server-ts": "^1.2.16",
        "typescript": "^3.9.7"
    },
    "devDependencies": {
        "@semantic-release/exec": "^5.0.0",
        "@semantic-release/github": "^7.2.2",
        "@semantic-release/npm": "^7.1.3",
        "@types/mocha": "^7.0.2",
        "@types/node": "^14.10.1",
        "body-parser": "^1.14.1",
        "husky": "^4.3.0",
        "jshint": "^2.12.0",
        "mocha": "^2.5.3",
        "semantic-release": "^17.4.2",
        "should": "^7.1.0",
        "sinon": "^1.14.1",
        "supertest": "^0.15.0"
    },
    "engines": {
        "node": ">=0.11"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run type-check"
        }
    }
}
