{
    "name": "oauth2-server-ts-express",
    "version": "1.2.16-develop.1",
    "description": "OAuth provider for express",
    "main": "dist/index.js",
    "files": [
        "dist",
        "index.ts"
    ],
    "scripts": {
        "clean:build": "rimraf ./dist",
        "build": "npx tsc -p tsconfig.build.json",
        "build:prod": "yarn clean:build && yarn build",
        "start-ex": "yarn copy:src-exam && yarn ex-redis",
        "copy:src-exam": "node ./CopyAndSyncSrcToExam.js",
        "ex-redis": "cd ./examples/redis && yarn && yarn start",
        "release": "semantic-release",
        "compile": "tsc",
        "type-check": "tsc --noEmit",
        "lint": "jshint .",
        "test": "mocha --recursive test"
    },
    "repository": {
        "type": "git",
        "url": "git://github.com/hoangnguyen1247/oauth2-server-ts-express"
    },
    "keywords": [
        "express",
        "oauth",
        "oauth2"
    ],
    "author": {
        "name": "Nuno Sousa",
        "email": "nfs@seegno.com"
    },
    "license": "SEE LICENSE IN LICENSE",
    "dependencies": {
        "bluebird": "^3.0.5",
        "express": "^4.13.3",
        "oauth2-server-ts": "^1.2.16",
        "typescript": "^3.9.7"
    },
    "devDependencies": {
        "@semantic-release/exec": "^5.0.0",
        "@semantic-release/github": "^7.2.2",
        "@semantic-release/npm": "^7.1.3",
        "@types/mocha": "^7.0.2",
        "@types/node": "^14.10.1",
        "body-parser": "^1.14.1",
        "copy-dir": "^1.3.0",
        "fs-extra": "^10.0.0",
        "husky": "^4.3.0",
        "jshint": "^2.12.0",
        "mocha": "^10.2.0",
        "rimraf": "^3.0.2",
        "semantic-release": "^19.0.3",
        "should": "^7.1.0",
        "sinon": "^1.14.1",
        "supertest": "^6.3.2"
    },
    "engines": {
        "node": ">=0.11"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run type-check"
        }
    }
}
